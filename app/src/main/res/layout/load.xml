<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:gravity="center"
	android:alpha="1.0"
	android:orientation="vertical">

	<LinearLayout
		android:layout_width="9999dp"
		android:layout_height="9999dp"
		android:translationY="0dp"
		android:gravity="center"
		android:background="#99000000"
		android:orientation="vertical">

		<LinearLayout
			android:orientation="horizontal"
			android:layout_width="250dp"
			android:layout_height="250dp"
			android:background="@drawable/app_jarvis"
			android:id="@+id/loadLinearLayout1"
			android:layout_marginTop="90dp"/>

		<LinearLayout
			android:orientation="vertical"
			android:layout_width="180dp"
			android:layout_height="45dp"
			android:layout_toRightOf="@id/loadLinearLayout1"
			android:background="@drawable/logo_psd"
			android:layout_marginTop="90dp"/>

	</LinearLayout>

</LinearLayout>

